{
    "manufacturer": "Philippe Payant",
    "keyboard_name": "handlebar",
    "maintainer": "Philippe Payant",
    "url": "",

    "development_board": "elite_c",

    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },

    "features": {
        "bootmagic": false,
        "command": false,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "rgblight": true,
        "nkro": false
    },

    "split": {
        "enabled": true,
        "transport": {
            "protocol": "i2c"
        },
        "usb_detect": {
            "enabled": true
        }
    },

    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "cols": ["F5", "F6", "F7", "B1", "B3"],
        "rows": ["D4", "C6", "D7", "E6", "B2"]
    },

    "layouts": {
        "LAYOUT_split_5x5": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 1.5},
                {"matrix": [0, 1], "x": 2, "y": 0},
                {"matrix": [0, 2], "x": 3, "y": 0},
                {"matrix": [0, 3], "x": 4, "y": 0},
                {"matrix": [0, 4], "x": 5, "y": 0},
                {"matrix": [1, 0], "x": 1, "y": 1.5},
                {"matrix": [1, 1], "x": 2, "y": 1},
                {"matrix": [1, 2], "x": 3, "y": 1},
                {"matrix": [1, 3], "x": 4, "y": 1},
                {"matrix": [1, 4], "x": 5, "y": 1},
                {"matrix": [2, 0], "x": 1, "y": 2.5},
                {"matrix": [2, 1], "x": 2, "y": 2},
                {"matrix": [2, 2], "x": 3, "y": 2},
                {"matrix": [2, 3], "x": 4, "y": 2},
                {"matrix": [2, 4], "x": 5, "y": 2},
                {"matrix": [3, 0], "x": 1, "y": 3.5},
                {"matrix": [3, 1], "x": 2, "y": 3},
                {"matrix": [3, 2], "x": 3, "y": 3},
                {"matrix": [3, 3], "x": 4, "y": 3},
                {"matrix": [3, 4], "x": 5, "y": 3},
                {"matrix": [4, 0], "x": 0, "y": 2.5},
                {"matrix": [4, 1], "x": 3.5, "y": 4},
                {"matrix": [4, 2], "x": 4.5, "y": 4},
                {"matrix": [4, 3], "x": 5.75, "y": 4.75},
                {"matrix": [4, 4], "x": 7, "y": 6}
            ]
        }
    },

    "caps_word": {
        "enabled": true,
        "both_shifts_turns_on": true
    },

    "rgblight": {
        "led_count": 2,
        "driver": "ws2812",
        "default": {
            "val": 35
        },
        "max_brightness": 85
    },

    "ws2812": {
        "driver": "bitbang",
        "pin": "B4"
    }
}
